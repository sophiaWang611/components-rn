{
  "name": "MSComponents",
  "version": "0.0.1",
  "private": true,
  "main": "MSComponents.js",
  "scripts": {
    "start": "node_modules/react-native/packager/packager.sh",
    "test": "PLATFORM=ios npm run mocha-test test/integration",
    "mocha-test": "mocha --recursive --compilers js:babel-register",
    "test-ios": "TARGET=test PLATFORM=ios node test/tasks/compile.js && PLATFORM=ios npm run test",
    "test-and": "TARGET=test PLATFORM=android node test/tasks/compile.js && PLATFORM=android npm run test",
    "test-all": "npm run test-ios && npm run test-and"
  },
