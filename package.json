    "react-native-scrollable-tab-view": "^0.3.7",
    "react-native-modal": "^0.4.0"
  },
  "devDependencies": {
    "appium": "1.4.13",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-plugin-syntax-async-functions": "^6.5.0",
    "babel-plugin-transform-class-properties": "^6.6.0",
